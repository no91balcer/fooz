{
  "apiVersion": 3,
  "name": "fooz-test/faq-item",
  "title": "FAQ Question",
  "category": "design",
  "icon": "editor-help",
  "description": "FAQ accordion with automatic numeration",
  "textdomain": "fooz-test",
  "supports": {
    "html": false,
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "attributes": {
    "question": {
      "type": "string",
      "source": "html",
      "selector": ".c-faq-item__question-inner"
    },
    "answer": {
      "type": "string",
      "source": "html",
      "selector": ".c-faq-item__answer-inner"
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "viewScript": "file:./view.js",
  "parent": [
    "fooz-test/faq-accordion"
  ]
}